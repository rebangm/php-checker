{
    "name": "rebangm/php-checker",
    "description": "tools to check repository with static analysis",
    "authors": [
        {
            "name": "Jean-philippe Depigny",
            "email": "jdepigny.ext@orange.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "phpunit/phpunit": "4.1.*",
        "phploc/phploc": "2.*",
        "phpunit/php-code-coverage": "2.*",
        "fabpot/php-cs-fixer": "0.5.*",
        "monolog/monolog": "1.10.*",
        "pdepend/pdepend": "2.0.*",
        "sebastian/phpcpd": "2.0.*",
        "squizlabs/php_codesniffer": "1.5.4"
    },
    "autoload": {
        "psr-4": {
            "Rebangm\\"       : "src/"
        }
    }
}
