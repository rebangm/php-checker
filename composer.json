{
    "name": "rebangm/php-checker",
    "description": "tools which perform static analysis on PHP project",
    "authors": [
        {
            "name": "Rebangm",
            "email": "rebangm@gmail.com"
        }
    ],
    "license": "MIT",
    "keywords": [
            "PHP",
            "Static analysis",
            "PHPLoc",
            "PHPCpd"
    ],
    "minimum-stability": "stable",
    "require": {
        "phpunit/phpunit": "~4.1",
        "phploc/phploc": "~2.0",
        "phpunit/php-code-coverage": "~2.0",
        "fabpot/php-cs-fixer": "~0.5",
        "monolog/monolog": "~1.10",
        "pdepend/pdepend": "~2.0",
        "sebastian/phpcpd": "~2.0",
        "squizlabs/php_codesniffer": "~1.5",
        "jakub-onderka/php-parallel-lint": "0.7",
        "sebastian/phpdcd": "~1.0",
        "halleck45/php-metrics": "~0.0"
    },
    "autoload": {
        "psr-4": {
            "Rebangm\\"       : "src/"
        }
    }
}
